{
    "editor.rulers": [80],
    "editor.fontFamily": "Andale Mono",
    "editor.fontSize": 12,
    "ruby.lint": {
        "rubocop": true,
        "reek": true
    },
    "editor.autoIndent": "full",
    "files.trimTrailingWhitespace": true,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderWhitespace": "none",
    "window.newWindowDimensions": "inherit",
    "window.restoreWindows": "all",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.panel.defaultLocation": "bottom",
    "workbench.editor.tabSizing": "shrink",
    "workbench.fontAliasing": "auto",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "mssql.connections": [
        {
            "server": "{{put-server-name-here}}",
            "database": "{{put-database-name-here}}",
            "user": "{{put-username-here}}",
            "password": ""
        }
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "editor.guides.indentation": true,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "gitlens.gitCommands.search.showResultsInSideBar": false,
    "[python]": {
        "editor.formatOnType": true
    },
    "markdown.extension.preview.autoShowPreviewToSide": true,
    "workbench.colorTheme": "Visual Studio Dark",
    "go.toolsManagement.autoUpdate": true,
    "editor.accessibilitySupport": "off",
    "gitlens.changes.locations": [
        "overview",
        "line",
        "gutter"
    ],
    "gitlens.fileAnnotations.preserveWhileEditing": false,
    "gitlens.codeLens.enabled": false,
    "explorer.confirmDelete": false,
    "application.shellEnvironmentResolutionTimeout": 30,
    "calva.autoOpenInspector": false,
}